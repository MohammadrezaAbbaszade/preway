<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle" android:useLevel="false">
            <corners android:radius="@dimen/grid_2" />
            <solid android:color="@color/black_alpha_30" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle" android:useLevel="true">
                <corners android:radius="@dimen/grid_2" />
                <solid android:color="@color/hq_purple" />
            </shape>
        </clip>
    </item>
</layer-list>
